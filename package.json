{
  "name": "chaufforsverksamheten",
  "version": "1.0.0",
  "description": "Full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "posttest": "npm run eslint",
    "eslint": "eslint .",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha 'test/**/*.js'",
    "docker-latest": "docker-compose run node_alpine npm test",
    "docker-10": "docker-compose run node_10_alpine npm test",
    "docker-8": "docker-compose run node_8_alpine npm test",
    "docker-6": "docker-compose run node_6_alpine npm test",
    "report-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "report-codecov": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "clean": "rm -rf npm-debug.log coverage .nyc_output",
    "preclean-all": "npm run clean",
    "clean-all": "rm -rf node_modules"
  },
  "author": "Martin Borg",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "popper.js": "^1.14.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codecov": "^3.1.0",
    "coveralls": "^3.0.2"
  }
}
